[
  {
    "name": "unknown type",
    "schema": "{\"afkable\": {\"type\": \"unknown\"}}",
    "expected_error": "unknown type"
  },
  {
    "name": "missing type",
    "schema": "{\"afkable\": {\"values\": [\"a\", \"b\"]}}",
    "expected_error": "missing required field 'type'"
  },
  {
    "name": "range missing max",
    "schema": "{\"spawn_rate\": {\"min\": 100, \"type\": \"range\"}}",
    "expected_error": "range type for key spawn_rate must have both 'min' and 'max'"
  },
  {
    "name": "range missing min",
    "schema": "{\"spawn_rate\": {\"max\": 500, \"type\": \"range\"}}",
    "expected_error": "range type for key spawn_rate must have both 'min' and 'max'"
  }
]
